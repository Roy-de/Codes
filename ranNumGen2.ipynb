    "#Lagged fibonacci generator\n",
    "x0 = 1\n",
    "x1 = 1\n",
    "m = 2**32\n",
    "for i in range(1,100):\n",
    "    x = np.mod((x1 + x0),m)\n",
    "    x0 = x1\n",
    "    x1 = x\n",
    "    print(x)"
   ]
