    "import numpy as np\n",
    "\n",
    "a = 2\n",
    "c = 4\n",
    "m = 5\n",
    "x = 3\n",
    "\n",
    "for i in range(1,17):\n",
    "    x = np.mod((a * x + c),m)\n",
    "    print(x)"
